                              </m:mtr>
                            </m:mtable>
                          </m:mfenced>
                        </m:mrow>
                      </m:mstyle>
                      <m:mrow/>
                    </m:mrow>
                    <m:annotation encoding="StarMath 5.0"> size 12{ left [ matrix {
3 {} # 4 {} ##
5 {} # 6{}
}  right ]} {}</m:annotation>
                  </m:semantics>
                </m:math>
                <media id="id1166889873263" alt=""><image src="../../media/graphics1-cb69.png" mime-type="image/png"/></media>
                <m:math>
                  <m:semantics>
                    <m:mrow>
                      <m:mstyle fontsize="12pt">
                        <m:mrow>
                          <m:mfenced open="[" close="]">
                            <m:mtable>
                              <m:mtr>
                                <m:mtd>
                                  <m:mrow>
                                    <m:mn>1</m:mn>
                                    <m:mrow/>
                                  </m:mrow>
                                </m:mtd>
                                <m:mtd>
                                  <m:mrow>
                                    <m:mn>0</m:mn>
                                    <m:mrow/>
