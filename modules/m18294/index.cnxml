                                  </m:mrow>
                                </m:mtd>
                              </m:mtr>
                            </m:mtable>
                          </m:mfenced>
                        </m:mrow>
                      </m:mstyle>
                      <m:mrow/>
                    </m:mrow>
                    <m:annotation encoding="StarMath 5.0"> size 12{ left [ matrix {
 - 3 {} # 2 {} ##
2 {  {1}  over  {2} }  {} #  - 1 {  {1}  over  {2} } {}
}  right ]} {}</m:annotation>
                  </m:semantics>
                </m:math>
                <m:math>
                  <m:semantics>
                    <m:mrow>
                      <m:mstyle fontsize="12pt">
                        <m:mrow>
                          <m:mfenced open="[" close="]">
                            <m:mtable>
                              <m:mtr>
                                <m:mtd>
                                  <m:mrow>
                                    <m:mn>3</m:mn>
                                    <m:mrow/>
                                  </m:mrow>
                                </m:mtd>
                                <m:mtd>
                                  <m:mrow>
                                    <m:mn>4</m:mn>
                                    <m:mrow/>
                                  </m:mrow>
                                </m:mtd>
                              </m:mtr>
                              <m:mtr>
